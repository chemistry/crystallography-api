{
    "name": "@chemistry/crystallography-web",
    "private": true,
    "version": "0.0.8",
    "description": "Crystallography Web",
    "author": {
        "name": "Volodymyr Vreshch",
        "url": "http://vreshch.com/"
    },
    "license": "MIT",
    "keywords": [
        "Application",
        "crystallography",
        "Git"
    ],
    "engines": {
        "node": ">=8.0.0"
    },
    "devEngines": {
        "node": ">=8.0.0"
    },
    "repository": "https://github.com/chemistry/crystallography-api/tree/master/packages/containers/crystallography-web",
    "scripts": {
        "deploy": "./deploy.sh",
        "build:backend": "tsc",
        "build:frontend": "webpack-cli --config webpack.config.js --mode production",
        "build": "rimraf dist/* && npm run build:backend && npm run build:frontend",
        "start": "node dist/backend/index.js",
        "serve": "webpack-dev-server  --config ./webpack.config.js",
        "app:serve": "npm run serve",
        "app:build": "npm run build",
        "app:start": "npm run start",
        "app:nodemon": "nodemon -e ts -w ./src -x ts-node src/index.ts",
        "app:build:frontend": "npm run build:frontend"
    },
    "dependencies": {
        "@google-cloud/firestore": "3.4.0",
        "@google-cloud/storage": "4.2.0",
        "@types/express": "4.17.2",
        "@types/node": "13.1.4",
        "@types/react-dom": "16.9.5",
        "@types/react-router-config": "5.0.1",
        "@types/react-router-dom": "5.1.3",
        "body-parser": "1.19.0",
        "connect-timeout": "1.9.0",
        "cross-env": "5.2.0",
        "express": "4.17.1",
        "react": "16.13.1",
        "react-dom": "16.10.2",
        "react-router": "5.1.2",
        "react-router-dom": "5.1.2",
        "spectre.css": "0.5.8"
    },
    "devDependencies": {
        "assets-webpack-plugin": "3.9.12",
        "awesome-typescript-loader": "5.2.1",
