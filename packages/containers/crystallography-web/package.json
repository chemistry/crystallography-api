        "mini-css-extract-plugin": "0.9.0",
        "node-sass": "4.13.0",
        "nodemon": "1.19.4",
        "supertest": "4.0.2",
        "sass-loader": "8.0.2",
        "style-loader": "1.1.3",
        "ts-node": "8.5.4",
        "typescript": "3.7.5",
        "webpack": "4.41.5",
        "webpack-cli": "3.3.10",
        "webpack-dev-server": "3.10.3"
    }
