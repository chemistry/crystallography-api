{
    "name": "@chemistry/application-web-manager",
    "private": true,
    "version": "0.0.5",
    "description": "Main COD Application",
    "author": {
        "name": "Volodymyr Vreshch",
        "url": "http://vreshch.com/"
    },
    "license": "MIT",
    "keywords": [
        "Web COD",
        "crystallography",
        "Git"
    ],
    "engines": {
        "node": ">=8.0.0"
    },
    "devEngines": {
        "node": ">=8.0.0"
    },
    "repository": "https://github.com/chemistry/crystallography-api/tree/master/packages/containers/application-web-manager",
    "scripts": {
        "deploy": "./deploy.sh",
        "build": "rimraf dist/* && npm run build:backend && npm run build:frontend",
        "build:backend": "tsc",
        "build:frontend": "webpack --config webpack.config.js --mode production",
        "start": "node dist/backend/index.js",
        "serve": "webpack-dev-server  --config webpack.config.js",
        "nodemon": "nodemon -e ts -w ./src -x ts-node src/backend/index.ts"
    },
    "dependencies": {
        "@types/express": "4.17.2",
        "@types/node": "13.1.4",
        "@types/react": "16.9.19",
        "@types/react-dom": "16.9.5",
        "@types/react-router-config": "5.0.1",
        "@types/react-router-dom": "5.1.3",
        "@chemistry/application-common": "0.0.5",
        "@chemistry/application-cod-search": "0.0.5",
        "express": "4.17.1",
        "react": "16.12.0",
        "react-dom": "16.12.0",
        "react-router": "5.1.2",
        "react-router-config": "5.1.1",
        "react-router-dom": "5.1.2"
    },
    "devDependencies": {
        "awesome-typescript-loader": "5.2.1",
        "html-webpack-plugin": "3.2.0",
        "nodemon": "1.19.4",
        "ts-node": "8.5.4",
        "typescript": "3.7.5",
        "webpack": "4.41.5",
        "webpack-cli": "3.3.10",
        "webpack-dev-server": "3.10.3"
    }
}
