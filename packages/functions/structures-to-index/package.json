{
    "name": "@chemistry/structures-to-index",
    "private": true,
    "version": "0.0.18",
    "description": "Store Structures to Indexing",
    "author": {
        "name": "Volodymyr Vreshch",
        "url": "http://vreshch.com/"
    },
    "license": "MIT",
    "keywords": [
        "COD syncronization",
        "cheminformatics",
        "Cloud functions"
    ],
    "engines": {
        "node": ">=8.0.0"
    },
    "devEngines": {
        "node": ">=8.0.0"
    },
    "repository": "https://github.com/chemistry/crystallography-api/tree/master/packages/functions/structures-to-index",
    "scripts": {
        "build": "rimraf dist/* && tsc && cp ./package.json ./dist && cp ./package-lock.json ./dist",
        "deploy": "gcloud functions deploy stuctures-to-index --entry-point handler --runtime nodejs12 --project crystallography-api --source ./dist --region europe-west1 --trigger-event providers/cloud.firestore/eventTypes/document.write --trigger-resource projects/crystallography-api/databases/\\(default\\)/documents/structures/{docId}",
        "create-index": "ts-node ./create-index.ts",
        "search": "ts-node ./search-example.ts"
    },
    "dependencies": {
        "@chemistry/common-functions": "0.0.18",
        "@types/elasticsearch": "5.0.36",
        "@google-cloud/firestore": "4.4.0",
        "elasticsearch": "16.7.1"
    },
    "devDependencies": {
        "ts-node": "9.0.0"
    }
}
