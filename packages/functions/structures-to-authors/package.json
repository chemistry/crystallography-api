{
    "name": "@chemistry/stuctures-to-authors",
    "private": true,
    "version": "0.0.18",
    "description": "Build authors table",
    "author": {
        "name": "Volodymyr Vreshch",
        "url": "http://vreshch.com/"
    },
    "license": "MIT",
    "keywords": [
        "COD syncronization",
        "cheminformatics",
        "Cloud functions"
    ],
    "engines": {
        "node": ">=8.0.0"
    },
    "devEngines": {
        "node": ">=8.0.0"
    },
    "repository": "https://github.com/chemistry/crystallography.io/tree/master/packages/functions/stuctures-to-authors",
    "scripts": {
        "build": "rimraf dist/* && tsc && cp ./package.json ./dist && cp ./package-lock.json ./dist",
        "deploy": "gcloud functions deploy stuctures-to-authors --entry-point handler --runtime nodejs10 --project crystallography-api --source ./dist --region europe-west1 --trigger-event providers/cloud.firestore/eventTypes/document.write --trigger-resource projects/crystallography-api/databases/\\(default\\)/documents/structures/{docId}",
        "check": "ts-node ./function.check.ts"
    },
    "dependencies": {
        "@google-cloud/firestore": "4.4.0"
    },
    "devDependencies": {
        "mock-cloud-firestore": "0.11.0",
        "ts-node": "8.10.2"
    }
}
