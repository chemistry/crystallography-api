{
    "name": "@chemistry/stuctures-to-catalog",
    "private": true,
    "version": "0.0.18",
    "description": "Build structures Catalog",
    "author": {
        "name": "Volodymyr Vreshch",
        "url": "http://vreshch.com/"
    },
    "license": "MIT",
    "keywords": [
        "COD syncronization",
        "cheminformatics",
        "Cloud functions"
    ],
    "engines": {
        "node": ">=8.0.0"
    },
    "devEngines": {
        "node": ">=8.0.0"
    },
    "repository": "https://github.com/chemistry/crystallography-api/tree/master/packages/functions/stuctures-to-catalog",
    "scripts": {
        "build": "rimraf dist/* && tsc && cp ./package.json ./dist && cp ./package-lock.json ./dist",
        "deploy": "gcloud functions deploy stuctures-to-catalog --entry-point handler --runtime nodejs12 --memory=1024MB --max-instances=1 --project crystallography-api --source ./dist --trigger-topic structures-to-catalog  --region europe-west1",
        "check": "ts-node ./function.check.ts"
    },
    "dependencies": {
        "@chemistry/common-functions": "0.0.18",
        "@google-cloud/firestore": "4.4.0"
    },
    "devDependencies": {
        "mock-cloud-firestore": "0.12.0",
        "ts-node": "9.0.0"
    }
}
