{
    "name": "@chemistry/functions-cod-sync-stop-instance",
    "private": true,
    "version": "0.0.1",
    "description": "Cloud functions that start COD sync instances by schedule",
    "author": {
        "name": "Volodymyr Vreshch",
        "url": "http://vreshch.com/"
    },
    "license": "MIT",
    "keywords": [
        "COD syncronization",
        "cheminformatics",
        "Cloud functions"
    ],
    "engines": {
        "node": ">=8.0.0"
    },
    "devEngines": {
        "node": ">=8.0.0"
    },
    "repository": "https://github.com/chemistry/crystallography-api/tree/master/packages/functions/cod-sync-stop-instance",
    "scripts": {
        "build": "rimraf dist/* && tsc && cp ./package.json ./dist",
        "deploy": "gcloud functions deploy codSyncStopInstance --runtime nodejs10 --project crystallography-api --source ./dist --trigger-topic cod-sync-schedule-instance-stop"
    },
    "dependencies": {
        "@chemistry/common-functions": "0.0.1",
        "@google-cloud/compute": "1.1.8"
    }
}
